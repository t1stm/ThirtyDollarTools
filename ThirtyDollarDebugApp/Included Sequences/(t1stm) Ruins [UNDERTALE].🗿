# Section: Drums |

#define(hihat)|
adofaicymbal@31.2|
#enddefine|

#define(kick)|
adofaikick|
#enddefine|

#define(snare)|
hammer|
#enddefine|

#define(stick)|
sidestick@1^-0.1|
#enddefine|

#define(crash)|
adofaicymbal|
#enddefine|

#define(o_hihat)|
adofaicymbal@12%70|!combine|ride2|
#enddefine|

# Section: Bass |

#define(f_bass)|
noteblock_bass@-4%133|
#enddefine|

#define(sf_bass)|
noteblock_bass@-4^-0.1%133|!combine|🎸@-4%33|
#enddefine|

#define(22_bass)|
#icut(noteblock_harp, 🤬)|
noteblock_harp@-28%50|!combine|🤬@-33.2%120|!combine|🤬@-45.2%120|
#enddefine|

#define(22_bass_sustain)|
#icut(🤬)|
🤬@-33.2%120|!combine|🤬@-45.2%120|
#enddefine|

#define(22_cut_bass)|
#icut(🤬)|
#enddefine|

# Section: Melody |

#define(sine)|
#icut(🤬)|
🤬@6.8%50|!combine|🤬@-5.2%50|
#enddefine|

#define(strings_sine)|
#icut(🤬, mariopaint_car)|
🤬@6.8%50|!combine|🤬@-5.2%50|!combine|mariopaint_car@12%15^-0.3|
#enddefine|

#define(cut_sine)|
#icut(🤬)|
#enddefine|

#define(cut_strings_sine)|
#icut(🤬, mariopaint_car)|
#enddefine|

#define(29_square_quad)|
noteblock_bit%65@0|noteblock_bit%63@0|noteblock_bit%60@0|
#enddefine|

#define(29_square_quad_m3)|
noteblock_bit%65@0|!combine|noteblock_bit%65@-3|noteblock_bit%63@0|!combine|noteblock_bit%63@-3|noteblock_bit%60@0|!combine|noteblock_bit%60@-3|
#enddefine|

