# Section: Notes |

#define(piano)|
noteblock_pling@0%15|
#enddefine|

#define(square_wave)|
mariopaint_gameboy@0%75|
#enddefine|

#define(piano_wave)|
mariopaint_gameboy@0%75|!combine|noteblock_pling@0%15^-0.1|!combine|mariopaint_luigi%15^0.3|
#enddefine|

# f#3 => 0|
#define(bass_line)|
#icut(fnf_left, fnf_right, mariopaint_car)|
fnf_right@-40%37.5|!combine|fnf_right@-28%42.5|!combine|fnf_left@-28%10.5|!combine|mariopaint_car@-20%25|
#enddefine|

#define(cut_bass)|
#icut(fnf_left, fnf_right, mariopaint_car)|
#enddefine|

#define(piano_chord)|
builttoscale@-2%80|!combine|builttoscale@10%90|
#enddefine|

# Section: Drums |

#define(hard_kick)|
adofaikick@0%100|!combine|🥁@0%100|
#enddefine|

#define(snare)|
karateman_throw@-2%99|!combine|noteblock_snare@-8%50|!combine|vvvvvv_flash@2%4|
#enddefine|

#define(hard_kick_alt)|
adofaikick@0%100^-0.1|!combine|🥁@0%100|
#enddefine|

#define(soft_kick)|
🥁^-0.1|!combine|hammer@-3%70^0.3|
#enddefine|

#define(snare_alt)|
noteblock_snare@-4%70|!combine|tab_rows%40|!combine|rdmistake@-3%20^-0.4|!combine|adofaicymbal@31%60^0.4|
#enddefine|

#define(hihat)|
karateman_throw@12%79|!combine|tab_rows@24%68^0.5|!combine|adofaicymbal@36^-0.3|
#enddefine|

# Section: Other |

#define(slide)|
noteblock_flute@-0.4%80|
#enddefine|

#define(slide_double)|
noteblock_flute@-0.2%45|noteblock_flute@-0.2%30^0.3|
#enddefine|

#define(slide_quiet)|
noteblock_flute@-0.4%50|
#enddefine|

#define(saw)|
amogus_emergency@0.4|!combine|amogus_emergency@-11.6%20|
#enddefine|

#define(cut_saw)|
#icut(amogus_emergency)|
#enddefine|

!speed@130|!speed@4@x|!cut|!divider|

!stop@8|!divider|

